import os
import sys
module_dir = os.path.dirname(os.path.abspath(__file__))
project_dir = os.path.abspath(os.path.join(module_dir, '..'))
sys.path.append(project_dir)

from rabin_karp_substring_search import rabin_karp_find_substring

def test_rabin_karp_find_substring():
    assert rabin_karp_find_substring('hello', 'll') == 2

def test_rabin_karp_find_substring_string_longer_than_substring():
    assert rabin_karp_find_substring('testing', 'ing') == 4

def test_rabin_karp_find_substring_not_found():
    assert rabin_karp_find_substring('python', 'java') == -1

def test_rabin_karp_find_substring_long_substring():
    assert rabin_karp_find_substring('testing', 'testingtesting') == -1

def test_rabin_karp_find_substring_empty_string():
    assert rabin_karp_find_substring('', 'substring') == -1

def test_rabin_karp_find_substring_empty_substring():
    assert rabin_karp_find_substring('abcde', '') == -1