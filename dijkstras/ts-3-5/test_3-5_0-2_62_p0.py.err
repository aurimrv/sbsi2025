import os
import sys
module_dir = os.path.dirname(os.path.abspath(__file__))
project_dir = os.path.abspath(os.path.join(module_dir, '..'))
sys.path.append(project_dir)

from dijkstras import dijkstras

def test_dijkstras():
    weighted_graph = {
        'A': {'B': 1, 'C': 4},
        'B': {'A': 1, 'C': 2, 'D': 5},
        'C': {'A': 4, 'B': 2, 'D': 1},
        'D': {'B': 5, 'C': 1}
    }

    assert dijkstras(weighted_graph, 'A', 'D') == ['A', 'C', 'D']

def test_dijkstras_multiple_paths():
    weighted_graph = {
        'A': {'B': 1, 'C': 4},
        'B': {'A': 1, 'C': 2, 'D': 5},
        'C': {'A': 4, 'B': 2, 'D': 1},
        'D': {'B': 5, 'C': 1, 'E': 2},
        'E': {'D': 2}
    }

    assert dijkstras(weighted_graph, 'A', 'E') == ['A', 'C', 'D', 'E']

def test_dijkstras_no_path():
    weighted_graph = {
        'A': {'B': 1, 'C': 4},
        'B': {'A': 1, 'C': 2},
        'C': {'A': 4, 'B': 2}
    }

    assert dijkstras(weighted_graph, 'A', 'D') == []

def test_dijkstras_same_source_dest():
    weighted_graph = {
        'A': {'B': 1, 'C': 4},
        'B': {'A': 1, 'C': 2},
        'C': {'A': 4, 'B': 2}
    }

    assert dijkstras(weighted_graph, 'A', 'A') == ['A']