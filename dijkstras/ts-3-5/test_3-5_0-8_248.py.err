import os
import sys
module_dir = os.path.dirname(os.path.abspath(__file__))
project_dir = os.path.abspath(os.path.join(module_dir, '..'))
sys.path.append(project_dir)

from dijkstras import dijkstras

def test_dijkstras():
    # Test case 1
    weighted_graph = {
        'A': {'B': 1, 'C': 4},
        'B': {'A': 1, 'C': 2, 'D': 5},
        'C': {'A': 4, 'B': 2, 'D': 1},
        'D': {'B': 5, 'C': 1}
    }
    src = 'A'
    dst = 'D'
    assert dijkstras(weighted_graph, src, dst) == ['A', 'B', 'C', 'D']

    # Test case 2
    weighted_graph = {
        'A': {'B': 3, 'C': 2},
        'B': {'A': 3, 'C': 1, 'D': 6},
        'C': {'A': 2, 'B': 1, 'D': 4},
        'D': {'B': 6, 'C': 4}
    }
    src = 'A'
    dst = 'D'
    assert dijkstras(weighted_graph, src, dst) == ['A', 'C', 'D']

    # Test case 3
    weighted_graph = {
        'A': {'B': 2, 'C': 5},
        'B': {'A': 2, 'C': 3, 'D': 7},
        'C': {'A': 5, 'B': 3, 'D': 4},
        'D': {'B': 7, 'C': 4}
    }
    src = 'A'
    dst = 'D'
    assert dijkstras(weighted_graph, src, dst) == ['A', 'B', 'C', 'D']

