import os
import sys
import pytest

module_dir = os.path.dirname(os.path.abspath(__file__))
project_dir = os.path.abspath(os.path.join(module_dir, '..'))
sys.path.append(project_dir)

from edit_distance import calculate_edit_distance

def test_calculate_edit_distance_match():
    str1 = "kitten"
    str2 = "sitting"
    expected = ['Substitute k', 'Substitute i', 'Match', 'Match', 'Match', 'Match', 'Match']
    assert calculate_edit_distance(str1, str2) == expected

def test_calculate_edit_distance_insert():
    str1 = "Saturday"
    str2 = "Sunday"
    expected = ['Substitute S', 'Match', 'Match', 'Match', 'Match', 'Match', 'Match', 'Match', 'Insert n']
    assert calculate_edit_distance(str1, str2) == expected

def test_calculate_edit_distance_delete():
    str1 = "hello"
    str2 = "hell"
    expected = ['Match', 'Match', 'Match', 'Match', 'Delete o']
    assert calculate_edit_distance(str1, str2) == expected

def test_calculate_edit_distance_empty_strings():
    str1 = ""
    str2 = ""
    expected = []
    assert calculate_edit_distance(str1, str2) == expected

