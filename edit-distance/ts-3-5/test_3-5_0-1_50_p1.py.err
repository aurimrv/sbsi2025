    assert calculate_edit_distance("aBcD123eFg", "xYz456uWv V") == ['Substitute x', 'Substitute Y', 'Substitute z', 'Substitute 4', 'Substitute 5', 'Substitute 6', 'Substitute u', 'Substitute W', 'Substitute v', 'Insert  ']
