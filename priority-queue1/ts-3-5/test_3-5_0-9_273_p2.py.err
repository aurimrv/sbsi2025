import os
import sys
from priority_queue1 import PriorityQueue

def test_priority_queue_add_task():
    pq = PriorityQueue()
    pq.add_task("Task1", 1)
    assert pq.pop_task() == "Task1"
    
    pq.add_task("Task2", 2)
    pq.add_task("Task3", 3)
    pq.add_task("Task4", 1)
    assert pq.pop_task() == "Task4"
    assert pq.pop_task() == "Task3"

def test_priority_queue_remove_task():
    pq = PriorityQueue()
    pq.add_task("Task1", 1)
    pq.add_task("Task2", 2)
    pq.remove_task("Task1")
    assert pq.pop_task() == "Task2"

def test_priority_queue_set_priority():
    pq = PriorityQueue()
    pq.add_task("Task1", 1)
    pq.add_task("Task2", 2)
    pq.set_priority("Task1", 3)
    assert pq.pop_task() == "Task1"

def test_priority_queue_pop_task():
    pq = PriorityQueue()
    pq.add_task("Task1", 1)
    pq.add_task("Task2", 2)
    pq.add_task("Task3", 3)
    assert pq.pop_task() == "Task3"
    assert pq.pop_task() == "Task2"
    assert pq.pop_task() == "Task1"

def test_priority_queue_pop_task_empty():
    pq = PriorityQueue()
    try:
        pq.pop_task()
    except KeyError as e:
        assert str(e) == "Pop from empty priority queue."